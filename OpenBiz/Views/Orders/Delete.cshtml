@model BLL.Entities.DistOrders.Order

@{
	Layout = null;
}

<!DOCTYPE html>

<html>
<head>
	<meta name="viewport" content="width=device-width" />
	<title>Delete</title>
</head>
<body>
    @using (Html.BeginForm())
    {
    	@Html.AntiForgeryToken()
    	<div class="modal-body">
    
    		<div class="form-horizontal">
    
    		<h3>Are you sure you want to delete this?</h3>
    		<div>
    			<h4>Order</h4>
    			<hr />
    			
    				
    		<div class="form-group">
    		
    			@Html.LabelFor(model => model.Distributor.Name, new { @class = "control-label col-sm-3" })
    
    
    		<div class="col-sm-9">
    			@Html.TextBoxFor(model => model.Distributor.Name, new { @class = "form-control required", @disabled = "disabled" })
    		</div>
    		
    		</div>
    
    	</div>
    				<div class="form-group">
    				<div class="col-sm-9 col-sm-offset-3">
    					<h3>
    						<span class="label label-danger">Are you sure you want to Delete this Order?
    						</span>
    					</h3>
    				</div>
    			</div>
    		</div>
    	</div>
    
    
    	<div class="modal-footer">
    		<button class="btn btn-primary" data-dismiss="modal">No</button>
    		<input class="btn btn-danger" type="submit" value="Yes" />
    	</div>
    
    	}
</body>
</html>
